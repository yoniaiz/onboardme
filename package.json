{
	"name": "onboardme",
	"version": "0.1.0",
	"description": "Gamified CLI for codebase onboarding",
	"module": "src/index.ts",
	"type": "module",
	"bin": {
		"onboardme": "dist/index.js"
	},
	"scripts": {
		"cli": "bun src/index.ts",
		"cli:init": "bun src/index.ts init",
		"cli:start": "bun src/index.ts start",
		"cli:status": "bun src/index.ts status",
		"cli:validate": "bun src/index.ts validate",
		"dev": "bun --hot src/index.ts",
		"dev:setup": "bun scripts/dev-setup.ts",
		"build": "bun build src/index.ts --outdir dist --target node",
		"typecheck": "tsc --noEmit",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"knip": "knip",
		"check": "biome check",
		"check:fix": "biome check --write"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/bun": "latest",
		"@types/react": "^19.2.10",
		"knip": "^5.81.0",
		"react-devtools-core": "^7.0.1"
	},
	"peerDependencies": {
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"chalk": "^5.6.2",
		"commander": "^14.0.3",
		"figlet": "^1.10.0",
		"ink": "^6.6.0",
		"ink-divider": "^4.1.1",
		"ink-select-input": "^6.2.0",
		"ink-text-input": "^6.0.0",
		"ora": "^9.1.0",
		"react": "^19.2.4",
		"zod": "^4.3.6"
	}
}
